{
  "name": "dot-api",
  "version": "0.0.5",
  "description": "An ultra readable api library",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "tsc && jest --runInBand"
  },
  "devDependencies": {
    "fetch-mock": "^6.5.2",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "node-fetch": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "axios": "^0.19.0",
    "typescript": "^3.0.1"
  },
  "author": "Matthew Beatty",
  "license": "MIT",
  "dependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/src/domain/**",
      "!**/vendor/**"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  }
}
